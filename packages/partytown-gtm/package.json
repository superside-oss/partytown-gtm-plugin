{
  "name": "@superside-oss/partytown-gtm",
  "version": "1.0.0-alpha.11",
  "license": "MIT",
  "author": "Slawek Kolodziej <slawek.kolodziej@superside.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/superside-oss/partytown-gtm-plugin"
  },
  "keywords": [
    "partytown",
    "google tag manager",
    "tag manager",
    "gtm"
  ],
  "publishConfig": {
    "access": "public"
  },
  "main": "./lib/index.cjs",
  "types": "./lib/index.d.ts",
  "exports": {
    "./util": {
      "import": {
        "types": "./lib/util.d.ts",
        "default": "./lib/util.mjs"
      },
      "require": {
        "types": "./lib/util.d.ts",
        "default": "./lib/util.cjs"
      }
    },
    "./script": {
      "import": {
        "types": "./lib/script.d.ts",
        "default": "./lib/script.mjs"
      },
      "require": {
        "types": "./lib/script.d.ts",
        "default": "./lib/script.cjs"
      }
    },
    ".": {
      "import": {
        "types": "./lib/index.d.ts",
        "default": "./lib/index.mjs"
      },
      "require": {
        "types": "./lib/index.d.ts",
        "default": "./lib/index.cjs"
      }
    }
  },
  "scripts": {
    "build.cjs": "node scripts/build -f cjs",
    "build.esm": "node scripts/build -f esm",
    "build.ts": "tsc",
    "build": "npm-run-all -l -p 'build.**'",
    "watch.cjs": "yarn build.cjs -w",
    "watch.esm": "yarn build.esm -w",
    "watch.ts": "tsc --watch --preserveWatchOutput",
    "watch": "npm-run-all -l -p 'watch.**'",
    "dev.cjs": "yarn build.cjs -w -d",
    "dev.esm": "yarn build.esm -w -d",
    "dev": "npm-run-all -l -p watch.ts 'dev.**'"
  },
  "devDependencies": {
    "commander": "^11.0.0",
    "esbuild": "^0.18.4",
    "npm-run-all": "^4.1.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "semver": "^7.5.4",
    "typescript": "^5.0.4"
  },
  "peerDependencies": {
    "react": "*",
    "react-dom": "*"
  },
  "packageManager": "yarn@3.5.1"
}